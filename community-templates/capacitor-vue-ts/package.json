{
  "name": "capacitor-vue-ts-template",
  "version": "1.0.0",
  "description": "Capacitor Mobile App Template with Vue and TypeScript",
  "type": "module",
  "scripts": {
    "build": "run-p type-check \"build-only {@}\" --",
    "build-only": "vite build",
    "dev": "vite",
    "preview": "vite preview",
    "sync": "capacitor sync",
    "add:ios": "capacitor add ios",
    "add:android": "capacitor add android",
    "run:ios": "capacitor run ios",
    "run:android": "capacitor run android",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@capacitor/android": "^5.0.0",
    "@capacitor/camera": "^5.0.0",
    "@capacitor/core": "^5.0.0",
    "@capacitor/device": "^5.0.0",
    "@capacitor/geolocation": "^5.0.0",
    "@capacitor/ios": "^5.0.0",
    "@capacitor/network": "^5.0.0",
    "@capacitor/status-bar": "^5.0.0",
    "@capacitor/storage": "^5.0.0",
    "@capacitor/toast": "^5.0.0",
    "vue": "^3.3.0",
    "@ionic/vue": "^7.0.0",
    "ionicons": "^7.0.0",
    "@ionic/core": "^7.0.0",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^5.0.0",
    "@rushstack/eslint-patch": "^1.3.0",
    "@tsconfig/node18": "^18.2.0",
    "@types/node": "^18.17.0",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "npm-run-all2": "^6.1.0",
    "prettier": "^3.0.0",
    "typescript": "~5.2.0",
    "vite": "^4.4.0",
    "vue-tsc": "^1.8.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": ["capacitor", "vue", "typescript", "mobile", "ios", "android"],
  "author": "Codiner",
  "license": "MIT"
}
