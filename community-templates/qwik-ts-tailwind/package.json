{
  "name": "qwik-ts-tailwind-template",
  "version": "1.0.0",
  "description": "Qwik Template with TypeScript and Tailwind",
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install new integrations'",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint src --ext ts,tsx,js,jsx",
    "preview": "qwik build preview && vite preview --open",
    "qwik": "qwik",
    "start": "vite --open --mode ssr"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.4.5",
    "@builder.io/qwik-city": "^1.4.5",
    "@types/eslint": "^8.56.0",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-qwik": "^1.4.5",
    "node-fetch": "^3.3.2",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3",
    "undici": "^6.2.1",
    "vite": "^5.0.8",
    "vite-tsconfig-paths": "^4.2.2"
  },
  "engines": {
    "node": ">=16.8.0 <18.0.0 || >=18.11"
  },
  "keywords": ["qwik", "typescript", "tailwindcss"],
  "author": "Codiner",
  "license": "MIT"
}
